$INSERT UT.INSERTS I_COMMON
EDITED.DATA = ICONV(INPUT.DATA, "MD4")
LOOP
WHILE (EDITED.DATA LT 1000) DO
   EDITED.DATA += LEN(A.ARG1)
REPEAT
VL.LIST.VAR1 = 'A' : @VM : "B" : @VM `C`

LABEL.FOR.GOSUB:

FOR_EACH ASSOCIATED LIST.VAR1 MATCHING "B"
   PRINT CHECKSUM(V.LIST.VAR1)
END_EACH LIST.VAR1

RETURN

CRT 'abc'[2]  ;* bleh
CRT FOO; CRT BAR

IF FOO THEN
   BLEH
END ELSE
   BLOOP
END

BEGIN CASE
   CASE FOO
      BAR
      CASE QUX
         BAZ
         CASE 1
         CASE 2
      END CASE
